new PlayerText:InvBoxTD[MAX_PLAYERS][2];
new PlayerText:Invboxname[MAX_PLAYERS];
new PlayerText:Invboxreceived[MAX_PLAYERS];
new PlayerText:Invboxmodel[MAX_PLAYERS];

CreateInvBoxTD(playerid, itemid, amount, status)
{
	if(pData[playerid][pInvBoxTimer] > 0)
	{
		KillTimer(pData[playerid][pInvBoxTimer]);
		pData[playerid][pInvBoxTimer] = 0;

		new string[128];
		new status1[128];

		if(status == 0)
		{
			status1 = "REMOVED";
		}
		else if(status == 1)
		{
			status1 = "RECEIVED";
		}

		format(string, sizeof(string), "%s", Itens[itemid][item_name]);
		PlayerTextDrawSetString(playerid, Invboxname[playerid], string);

		format(string, sizeof(string), "%s %ix", status1, amount);
		PlayerTextDrawSetString(playerid, Invboxreceived[playerid], string);

		PlayerTextDrawSetPreviewModel(playerid, Invboxmodel[playerid], Itens[itemid][item_model]);
		PlayerTextDrawSetPreviewRot(playerid, Invboxmodel[playerid], Itens[itemid][item_previewrot][0], Itens[itemid][item_previewrot][1], Itens[itemid][item_previewrot][2], Itens[itemid][item_previewrot][3]);

		PlayerTextDrawHide(playerid, Invboxname[playerid]);
		PlayerTextDrawHide(playerid, Invboxreceived[playerid]);
		PlayerTextDrawHide(playerid, Invboxmodel[playerid]);
		PlayerTextDrawShow(playerid, Invboxname[playerid]);
		PlayerTextDrawShow(playerid, Invboxreceived[playerid]);
		PlayerTextDrawShow(playerid, Invboxmodel[playerid]);

		pData[playerid][pInvBoxTimer] = SetTimerEx("HideInventoryBox", 3000, false, "i", playerid);
		return 1;
	}
	else 
	{
		new string[128];
		new status1[128];

		if(status == 0)
		{
			status1 = "REMOVED";
		}
		else if(status == 1)
		{
			status1 = "RECEIVED";
		}

		format(string, sizeof(string), "%s", Itens[itemid][item_name]);
		PlayerTextDrawSetString(playerid, Invboxname[playerid], string);

		format(string, sizeof(string), "%s %ix", status1, amount);
		PlayerTextDrawSetString(playerid, Invboxreceived[playerid], string);

		PlayerTextDrawSetPreviewModel(playerid, Invboxmodel[playerid], Itens[itemid][item_model]);
		PlayerTextDrawSetPreviewRot(playerid, Invboxmodel[playerid], Itens[itemid][item_previewrot][0], Itens[itemid][item_previewrot][1], Itens[itemid][item_previewrot][2], Itens[itemid][item_previewrot][3]);

		PlayerTextDrawShow(playerid, InvBoxTD[playerid][0]);
		PlayerTextDrawShow(playerid, InvBoxTD[playerid][1]);
		PlayerTextDrawShow(playerid, Invboxname[playerid]);
		PlayerTextDrawShow(playerid, Invboxreceived[playerid]);
		PlayerTextDrawShow(playerid, Invboxmodel[playerid]);

		pData[playerid][pInvBoxTimer] = SetTimerEx("HideInventoryBox", 3000, false, "i", playerid);
		return 1;
	}
}

